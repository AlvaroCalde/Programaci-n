Proceso UD03A024
	Definir num1, num2, num3 Como Entero;
	Escribir "Introduzca el primer número" Sin Saltar;
	Leer num1;
	Escribir "Introduzca el segundo número" Sin Saltar;
	Leer num2;
	Escribir "Introduzca el tercer número" Sin Saltar;
	Leer num3;
	
	Si num1-num2=num2-num3  Entonces
		Escribir "Forman progresión arimétrica";
	SiNo
		Escribir "No forman progresión aritmétrica";
	FinSi
	
FinProceso
